    <url-pattern>/search/MashupTagRelationToJson</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApiTagRelationToJsonServlet</servlet-name>
    <url-pattern>/search/ApiTagRelationToJson</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Statistics2Servlet</servlet-name>
    <url-pattern>/search/statistics/Statistics2Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Statistics3Servlet</servlet-name>
    <url-pattern>/search/statistics/Statistics3Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Statistics4Servlet</servlet-name>
    <url-pattern>/search/statistics/Statistics4Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Statistics5Servlet</servlet-name>
    <url-pattern>/search/statistics/Statistics5Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Statistics6Servlet</servlet-name>
    <url-pattern>/search/statistics/Statistics6Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RelatedCharacteristicsIndicatorsServlet</servlet-name>
    <url-pattern>/search/RelatedCharacteristicsIndicatorsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AnalyzeDataDownLoadServlet</servlet-name>
    <url-pattern>/search/AnalyzeDataDownLoad</url-pattern>
  </servlet-mapping>

  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>